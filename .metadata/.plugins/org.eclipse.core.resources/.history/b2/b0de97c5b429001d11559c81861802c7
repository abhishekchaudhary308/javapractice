package tablebooking;

import java.text.SimpleDateFormat;

public class Mains {

	public static void main(String[] args) throws ParseException {

        SimpleDateFormat formatter = new SimpleDateFormat("dd-MM-yyyy");
           Customer c1 = new Customer(101l,"abc","8646477388",formatter.parse("12-12-1990"), 12000.0,24000.0,formatter.parse("12-12-2012") , 4.0);
           c1.display();
           System.out.println(" ");
           Customer c2 = new Customer(102l,"abc","8646477388",formatter.parse("12-12-1991"), 10000.0,22000.0,formatter.parse("12-12-2013"), 5.0);
           c2.display();
           System.out.println(" ");
           System.out.println("Are these customers equal? " + (c1.equals(c2)));
   }
}
