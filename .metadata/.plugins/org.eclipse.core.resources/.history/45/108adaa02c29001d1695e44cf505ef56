package tablebooking;


public class ContactBo {
	
	

	public static Customer findCustomerByName(Customer[] customers, String name)
	{
		
		  for(int i= 0; i<customers.length;i++)
		  {
			  if(customers[i].getName().equals(name))
			  {
				  return customers[i];
			  }
		  }
		
		
		return null;
	}	
	
}

